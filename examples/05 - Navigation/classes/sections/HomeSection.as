package sections{	import asf.core.elements.Section;	import asf.core.util.Sequence;	import asf.interfaces.ISectionView;	import asf.view.UIView;	import view.HomeSectionView;	import asf.core.viewcontrollers.InOutViewController;	import asf.interfaces.ISequence;		public class HomeSection extends HomeSectionView implements ISectionView	{		private var controller:InOutViewController;				public function HomeSection( )		{			controller = new InOutViewController( this );		}				public function init(p_section:Section, ...parameters):void		{		}				public function open(p_delay:uint=0):ISequence		{			return controller.animateIn( );		}				public function close(p_delay:uint=0):ISequence		{			return controller.animateOut( );		}				public function dispose():void		{			controller.dispose( );			controller = null;		}	}}